{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "comment": "OCPP 2.0 - v1p0",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "status": {
      "type": "string",
      "additionalProperties": true,
      "enum": [
        "BadMessage",
        "Idle",
        "NotSupportedOperation",
        "PermissionDenied",
        "Uploaded",
        "UploadFailure",
        "Uploading"
      ]
    },
    "requestId": {
      "type": "integer"
    }
  },
  "required": [
    "status",
    "requestId"
  ]
}