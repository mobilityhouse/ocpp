{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "comment": "OCPP 2.0 - v1p0",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "currentTime": {
      "type": "string",
      "format": "date-time"
    },
    "interval": {
      "type": "integer"
    },
    "status": {
      "type": "string",
      "additionalProperties": true,
      "enum": [
        "Accepted",
        "Pending",
        "Rejected"
      ]
    }
  },
  "required": [
    "currentTime",
    "interval",
    "status"
  ]
}
