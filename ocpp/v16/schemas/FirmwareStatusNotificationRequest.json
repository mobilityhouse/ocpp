{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "FirmwareStatusNotificationRequest",
    "type": "object",
    "properties": {
        "status": {
            "type": "string",
            "additionalProperties": false,
            "enum": [
                "Downloaded",
                "DownloadFailed",
                "Downloading",
                "Idle",
                "InstallationFailed",
                "Installing",
                "Installed"
            ]
        }
    },
    "additionalProperties": false,
    "required": [
        "status"
    ]
}
